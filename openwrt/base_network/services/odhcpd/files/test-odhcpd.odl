%populate {
    object ProcessMonitor.Test {
        instance add ("odhcpd") {
            parameter Name = "odhcpd";
            parameter Type = "Process";
            parameter Subject = "/var/run/odhcpd.pid";
            parameter MaxFailNum = 2;
            parameter MaxFailDuration = -1;
            parameter FailAction = "RESTART";
            parameter RebootAfterRestartThreshold = 3;
            parameter TestInterval = 30;
            parameter TestIntervalMultiplier = 1;
            parameter TestResetInterval = 300;
        }
    }
}
